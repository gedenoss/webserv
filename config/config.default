server {
    listen 8000;
	server_name localhost;
    host 127.0.0.1;
    root indexx;
    client_max_body_size 10M;
	index index.html;

    location / {
        allow_methods  DELETE POST GET;
        autoindex off;
    }
    location /admin {
        allow_methods POST GET;
    }

    location /images {
        allow_methods POST GET;
        
    }
}

