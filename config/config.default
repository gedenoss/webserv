server {
    listen 8QS0;
    server_name localhost;
    root /var/www/html;
    client_max_body_size 10M;
    error_page 404 /static/404.html;

    location / {             
        allow_methods GET POST;  
        index index.html;
        autoindex off;
    }

    location /upload {
        allow_methods POST;
        upload_dir /var/www/uploads;
    }

    location /cgi-bin {
        allow_methods GET POST;
        cgi_extension .php;
        cgi_path /usr/bin/php-cgi;
    }
}